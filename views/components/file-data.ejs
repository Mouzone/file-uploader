<img src="/data/uploads<%= relativeRoute %>" alt=<%= relativeRoute %>>
<div id="info">
    <p> <%= size %> </p>
    <p> <%= uploadTime %> </p>
    <% if (shareExpiration) {%>
        <div id="share-info">
            <p> Shared until: <%= shareExpiration %></p>
            <form action="/file/<%= id %>/unshare" method="POST">
                <button> Unshare </button>
            </form>
        </div>
    <% } %>
</div>